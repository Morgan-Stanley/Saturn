  <!-- CustomLink -->
  <xs:complexType name="CustomLink" ecore:name="CustomLink" >
    <xs:complexContent>
      <xs:extension base="saturn:AbstractLink">
        <xs:sequence>          
          <xs:element name="Code" minOccurs="0" maxOccurs="1" type="value:complexValue" />
        </xs:sequence>
        <xs:attribute name="Description" type="xs:string" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="AbstractResourceLink" abstract="true">
    <xs:attribute ecore:reference="type:NamedResourceDefinition"        name="source" type="xs:anyURI" />
    <xs:attribute ecore:reference="referenceResource:ReferenceResource" name="target" type="xs:anyURI" />
  </xs:complexType>

  <xs:complexType name="ResourceLink">
    <xs:complexContent>
      <xs:extension base="saturn:AbstractResourceLink" />
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResourceStepLink">
    <xs:complexContent>
      <xs:extension base="saturn:AbstractResourceLink">
        <xs:attribute ecore:reference="saturn:AbstractStep" name="targetStep" type="xs:anyURI" />
        <xs:attribute name="description" type="xs:string" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResourceNestedLink">
    <xs:complexContent>
      <xs:extension base="saturn:AbstractResourceLink">
        <xs:attribute ecore:reference="type:NamedResourceDefinition" name="targetEnclosingResource" type="xs:anyURI" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

