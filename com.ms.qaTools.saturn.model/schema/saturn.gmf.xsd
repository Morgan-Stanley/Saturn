  <xs:element name="Saturn">
    <xs:complexType ecore:name="Saturn" >    
      <xs:complexContent>
        <xs:extension base="saturn:RunGroup">
          <xs:sequence>
            <xs:element name="WorkDir"           type="value:complexValue" minOccurs="0" maxOccurs="1" />
            <xs:element name="RunNumberFileName" type="value:complexValue" minOccurs="0" maxOccurs="1" />
            <xs:element name="DefaultRunNumber"  type="value:complexValue" minOccurs="0" maxOccurs="1" />
            
            <!-- ReportingConfigurations -->
            <xs:element name="PreExecutionListeners"  minOccurs="0" maxOccurs="unbounded" type="type:AbstractListener" />
            <xs:element name="Listeners"              minOccurs="0" maxOccurs="unbounded" type="type:AbstractListener" />
            <xs:element name="PostExecutionListeners" minOccurs="0" maxOccurs="unbounded" type="type:AbstractListener" />
                  
            <xs:element minOccurs="0" maxOccurs="unbounded" name="Links"         type="saturn:AbstractLink" />
            <xs:element minOccurs="0" maxOccurs="unbounded" name="ResourceLinks" type="saturn:AbstractResourceLink" ecore:transient="true" />
          </xs:sequence>
          <xs:attribute name="version" type="xs:token" default="2.1.11" />
        </xs:extension>  
      </xs:complexContent>
    </xs:complexType>
    
    <xs:unique name="uniqueIncludeFileNames">
      <xs:selector xpath="saturn:IncludeFiles" />
      <xs:field xpath="@name" />
    </xs:unique>    
  </xs:element>	
  <!-- AbstractLink -->
  <xs:complexType name="AbstractLink" ecore:name="AbstractLink" abstract="true" ecore:abstract="true">
    <xs:attribute ecore:reference="saturn:AbstractRunGroup" name="source" type="xs:anyURI" />
    <xs:attribute ecore:reference="saturn:AbstractRunGroup" name="target" type="xs:anyURI" />
  </xs:complexType>
  
  <!-- OnPassLinkType -->
  <xs:complexType name="OnPassLink" ecore:name="OnPassLink" >
    <xs:complexContent>
      <xs:extension base="saturn:AbstractLink"/>
    </xs:complexContent>
  </xs:complexType>

  <!-- OnFailLink -->
  <xs:complexType name="OnFailLink" ecore:name="OnFailLink" >
    <xs:complexContent>
