  <!-- AbstractRunGroup -->
  <xs:complexType name="AbstractRunGroup" abstract="true" ecore:name="AbstractRunGroup" ecore:abstract="true" >
   	<xs:sequence>
   	  <xs:element name="Imports" minOccurs="0" maxOccurs="unbounded" >
   	    <xs:complexType ecore:name="RunGroupImport" >
   	      <xs:sequence/>
   	      <xs:attribute name="includeFile" type="xs:token"                  />
   	      <xs:attribute name="runGroup"    type="xs:token"                  />
   	      <xs:attribute name="attributes"  type="xs:boolean" default="true" />
   	      <xs:attribute name="resources"   type="xs:boolean" default="true" />
   	      <xs:attribute name="enabled"     type="xs:boolean" default="true" />
   	    </xs:complexType>
      </xs:element>
      <xs:element ref="exception:Exception" minOccurs="0" />
   	  <xs:element name="Resources"          minOccurs="0" maxOccurs="unbounded" type="type:NamedResourceDefinition"   />
   	  <xs:element name="Attributes"         minOccurs="0" maxOccurs="unbounded" type="saturn:Attribute"               />
   	  <xs:element name="Annotations"        minOccurs="0" maxOccurs="unbounded" type="type:AbstractAnnotation"        />
   	  <xs:element name="RepetitionHandler"  minOccurs="0" maxOccurs="1"         type="type:AbstractRepetitionHandler" />
      <xs:element name="Modifiers"          minOccurs="0" maxOccurs="unbounded"         type="type:AbstractRunGroupModifier"  />
      <xs:element name="Parameters"         minOccurs="0" maxOccurs="unbounded" type="saturn:AbstractParameter" />
      <xs:element name="IncludeFiles" minOccurs="0" maxOccurs="unbounded" type="saturn:IncludeFile" />
      <xs:element name="AlmId" minOccurs="0" maxOccurs="1" type="type:AlmId" />
    </xs:sequence>      	
    <xs:attribute name="name"            type="xs:string" use="optional" />
    <xs:attribute name="id"              type="xs:string" use="optional" />
    <xs:attribute name="enabled"         type="xs:boolean"                      default="true"     use="optional" />
    <xs:attribute name="warnOnFailure"   type="xs:boolean"                      default="false"    use="optional" />
    <xs:attribute name="waitBefore"      type="xs:integer"                      default="0" />
    <xs:attribute name="waitAfter"       type="xs:integer"                      default="0" />
    <xs:attribute name="defaultStatus"   type="type:SaturnStatusEnum"           default="FAIL"     use="optional" />
    <xs:attribute name="statusCondition" type="type:SaturnStatusConditionEnum"  default="ANY FAIL" use="optional" />
    <xs:attribute name="procedure"       type="xs:boolean"                      default="false"    use="optional" />

    <xs:attribute name="canMultiplex"              type="xs:boolean" default="false" use="optional" />
    <xs:attribute name="maxSimultaneousIterations" type="xs:integer" default="10"    use="optional" />

  </xs:complexType>  
  
  <xs:complexType name="PerlRunGroupModifier" >
    <xs:complexContent>    
      <xs:extension base="type:AbstractRunGroupModifier">
        <xs:sequence>
